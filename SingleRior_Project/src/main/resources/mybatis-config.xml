<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		 <setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

	<typeAliases> 
		<typeAlias type="com.E1I4.project.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.E1I4.project.marketBoard.model.vo.MarketBoard" alias="MarketBoard"/>
		<typeAlias type="com.E1I4.project.notiBoard.model.vo.NotiBoard" alias="NotiBoard"/>
		<typeAlias type="com.E1I4.project.storeBoard.model.vo.StoreBoard" alias="StoreBoard"/>
		<typeAlias type="com.E1I4.project.commuBoard.model.vo.CommuBoard" alias="CommuBoard"/>
		<typeAlias type="com.E1I4.project.common.model.vo.Attachment" alias="Attachment"/>
		<typeAlias type="com.E1I4.project.common.model.vo.Product" alias="Product"/>
		<typeAlias type="com.E1I4.project.common.model.vo.WishList" alias="WishList"/>
		<typeAlias type="com.E1I4.project.common.model.vo.Reply" alias="Reply"/>
		<typeAlias type="com.E1I4.project.common.model.vo.ReReply" alias="ReReply"/>
		<typeAlias type="com.E1I4.project.common.model.vo.Board" alias="Board"/>

	</typeAliases>

	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
	   	<mapper resource="mappers/market-mapper.xml"/>
	   	<mapper resource="mappers/store-mapper.xml"/>
	   	<mapper resource="mappers/noti-mapper.xml"/>
	   	<mapper resource="mappers/commu-mapper.xml"/>
	   	<mapper resource="mappers/admin-mapper.xml"/>
	</mappers>
</configuration>
